{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["ES5"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "lib",
    "strictNullChecks": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "ES5"
  }
}